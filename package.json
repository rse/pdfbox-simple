{
    "name":        "pdfbox-simple",
    "version":     "1.3.1",
    "description": "Simple Apache PDFBox Wrapper",
    "keywords":    [ "pdfbox", "wrapper" ],
    "main":        "./pdfbox-api.js",
    "bin": {
        "pdfbox":  "./pdfbox-cli.js"
    },
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/pdfbox-simple.git"
    },
    "author": {
        "name":  "Dr. Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "MIT",
    "homepage": "https://github.com/rse/pdfbox-simple",
    "bugs":     "https://github.com/rse/pdfbox-simple/issues",
    "devDependencies": {
        "eslint":                  "8.38.0",
        "eslint-config-standard":  "17.0.0",
        "eslint-plugin-import":    "2.27.5",
        "eslint-plugin-node":      "11.1.0",
        "tslint":                  "6.1.3",
        "typescript":              "5.0.4"
    },
    "dependencies": {
        "npm-install-fetch":       "1.3.7",
        "which":                   "3.0.0",
        "eventemitter3":           "5.0.0",
        "execa":                   "5.1.1"
    },
    "upd":                         [ "!execa" ],
    "engines": {
        "node":                    ">=14.0.0"
    },
    "scripts": {
        "prepublishOnly": "eslint --config eslint.yaml pdfbox-*.js && tslint --config tslint.json pdfbox-*.d.ts",
        "install":        "npm-install-fetch",
        "test":           "node sample.js"
    },
    "npm-install-fetch": {
        "name":    "Apache PDFBox 2.0.28 [8 MB]",
        "input":   "http://archive.apache.org/dist/pdfbox/2.0.28/pdfbox-app-2.0.28.jar",
        "output":  "pdfbox-cli.jar"
    }
}
