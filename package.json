{
    "name":        "pdfbox-simple",
    "version":     "0.9.0",
    "description": "Simple Apache PDFBox Wrapper",
    "keywords":    [ "pdfbox", "wrapper" ],
    "main":        "./pdfbox-api.js",
    "bin": {
        "pdfbox":  "./pdfbox-cli.js"
    },
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/pdfbox-simple.git"
    },
    "author": {
        "name":  "Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "MIT",
    "homepage": "https://github.com/rse/pdfbox-simple",
    "bugs":     "https://github.com/rse/pdfbox-simple/issues",
    "devDependencies": {
        "babel-eslint":            "8.2.6",
        "eslint":                  "5.3.0",
        "eslint-config-standard":  "11.0.0",
        "eslint-plugin-standard":  "3.1.0",
        "eslint-plugin-promise":   "3.8.0",
        "eslint-plugin-import":    "2.13.0",
        "eslint-plugin-node":      "7.0.1",
        "tslint":                  "5.11.0",
        "typescript":              "3.0.1"
    },
    "dependencies": {
        "npm-install-fetch":       "1.0.3",
        "which":                   "1.3.1",
        "mz":                      "2.7.0",
        "eventemitter3":           "3.1.0",
        "execa":                   "0.10.0"
    },
    "engines": {
        "node":                    ">=8.0.0"
    },
    "scripts": {
        "prepublishOnly": "eslint --config eslint.yaml pdfbox-simple-*.js && tslint --config tslint.json pdfbox-simple-api.d.ts",
        "install":        "npm-install-fetch",
        "test":           "node sample.js"
    },
    "npm-install-fetch": {
        "name":    "Apache PDFBox 2.0.11 [8 MB]",
        "input":   "http://www-eu.apache.org/dist/pdfbox/2.0.11/pdfbox-app-2.0.11.jar",
        "output":  "pdfbox-cli.jar"
    }
}
