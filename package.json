{
    "name":        "pdfbox-simple",
    "version":     "1.0.3",
    "description": "Simple Apache PDFBox Wrapper",
    "keywords":    [ "pdfbox", "wrapper" ],
    "main":        "./pdfbox-api.js",
    "bin": {
        "pdfbox":  "./pdfbox-cli.js"
    },
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/pdfbox-simple.git"
    },
    "author": {
        "name":  "Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "MIT",
    "homepage": "https://github.com/rse/pdfbox-simple",
    "bugs":     "https://github.com/rse/pdfbox-simple/issues",
    "devDependencies": {
        "babel-eslint":            "10.0.1",
        "eslint":                  "5.16.0",
        "eslint-config-standard":  "12.0.0",
        "eslint-plugin-standard":  "4.0.0",
        "eslint-plugin-promise":   "4.1.1",
        "eslint-plugin-import":    "2.17.3",
        "eslint-plugin-node":      "9.1.0",
        "tslint":                  "5.17.0",
        "typescript":              "3.5.2"
    },
    "dependencies": {
        "npm-install-fetch":       "1.0.7",
        "which":                   "1.3.1",
        "eventemitter3":           "3.1.2",
        "execa":                   "1.0.0"
    },
    "engines": {
        "node":                    ">=8.0.0"
    },
    "scripts": {
        "prepublishOnly": "eslint --config eslint.yaml pdfbox-*.js && tslint --config tslint.json pdfbox-*.d.ts",
        "install":        "npm-install-fetch",
        "test":           "node sample.js"
    },
    "npm-install-fetch": {
        "name":    "Apache PDFBox 2.0.15 [8 MB]",
        "input":   "http://www-eu.apache.org/dist/pdfbox/2.0.15/pdfbox-app-2.0.15.jar",
        "output":  "pdfbox-cli.jar"
    }
}
