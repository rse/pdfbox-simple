{
    "name":        "pdfbox-simple",
    "version":     "1.2.7",
    "description": "Simple Apache PDFBox Wrapper",
    "keywords":    [ "pdfbox", "wrapper" ],
    "main":        "./pdfbox-api.js",
    "bin": {
        "pdfbox":  "./pdfbox-cli.js"
    },
    "repository": {
        "type": "git",
        "url":  "https://github.com/rse/pdfbox-simple.git"
    },
    "author": {
        "name":  "Dr. Ralf S. Engelschall",
        "email": "rse@engelschall.com",
        "url":   "http://engelschall.com"
    },
    "license":  "MIT",
    "homepage": "https://github.com/rse/pdfbox-simple",
    "bugs":     "https://github.com/rse/pdfbox-simple/issues",
    "devDependencies": {
        "babel-eslint":            "10.1.0",
        "eslint":                  "7.7.0",
        "eslint-config-standard":  "14.1.1",
        "eslint-plugin-standard":  "4.0.1",
        "eslint-plugin-promise":   "4.2.1",
        "eslint-plugin-import":    "2.22.0",
        "eslint-plugin-node":      "11.1.0",
        "tslint":                  "6.1.3",
        "typescript":              "4.0.2"
    },
    "dependencies": {
        "npm-install-fetch":       "1.2.5",
        "which":                   "2.0.2",
        "eventemitter3":           "4.0.7",
        "execa":                   "4.0.3"
    },
    "engines": {
        "node":                    ">=10.0.0"
    },
    "scripts": {
        "prepublishOnly": "eslint --config eslint.yaml pdfbox-*.js && tslint --config tslint.json pdfbox-*.d.ts",
        "install":        "npm-install-fetch",
        "test":           "node sample.js"
    },
    "npm-install-fetch": {
        "name":    "Apache PDFBox 2.0.21 [8 MB]",
        "input":   "http://archive.apache.org/dist/pdfbox/2.0.21/pdfbox-app-2.0.21.jar",
        "output":  "pdfbox-cli.jar"
    }
}
